<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/listFragment">

    <fragment
        android:id="@+id/listFragment"
        android:name="com.test.stfalcontest.view.list.ListFragment"
        android:label="fragment_list"
        tools:layout="@layout/fragment_list" >
        <action
            android:id="@+id/action_listFragment_to_detailsFragment"
            app:destination="@id/detailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailsFragment"
        android:name="com.test.stfalcontest.view.details.DetailsFragment"
        android:label="DetailsFragment" >
        <argument
            android:name="detailsList"
            app:argType="com.test.stfalcontest.view.details.DetailVM[]" />
    </fragment>

    <!--    <action-->
    <!--        android:id="@+id/action_modesFragment_to_gameFragment"-->
    <!--        app:destination="@id/gameFragment" />-->

</navigation>
